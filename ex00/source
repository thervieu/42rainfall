#define _GNU_SOURCE
#include <unistd.h>
#include <string.h>
#include <stdio.h>

int main(int ac, char **av) {
	char *args[2];
	int id = atoi(av[1]);
	if (id == 423) {
		args[0] = "/bin/sh";
		args[1] = NULL;
		setresgid(getegid(), getegid(), getegid());
		setresuid(geteuid(), geteuid(), geteuid());
		execv("/bin/sh", args);
	}
	fwrite("No !\n", 1, 5, stderr);
}
